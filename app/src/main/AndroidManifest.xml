<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xbb.smartscene">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SmartSceneActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SceneSettingActivity" />
        <activity
            android:name=".AlarmSettingActivity"
            android:label="@string/alarm_title"/>
        <provider
            android:authorities="com.xbb.smartscene"
            android:name="com.xbb.provider.SceneContenProvider" />
    </application>

</manifest>
